{
	"schemaVersion": 1,
	"id": "custom-mob-spawns",
	"version": "${version}",
	"name": "Custom Mob Spawns",
	"description": "Simple configurable mob spawning.\nFork of Custom Spawns that supports 1.20 and beyond.\nThis particular build supports every versions between 1.19.3 and 1.20.5 under Fabric.",
	"authors": [
		"AzyLooper"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/custom-mob-spawns/",
		"sources": "https://github.com/AzyLooper/custom-mob-spawns",
		"issues": "https://github.com/AzyLooper/custom-mob-spawns/issues"
	},
	"license": "MIT",
	"icon": "assets/custom-mob-spawns/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mod.azylooper.custommobspawns.CustomMobSpawns"
		],
		"modmenu": [
			"mod.azylooper.custommobspawns.config.CustomMobSpawnModMenu"
		]
	},
	"mixins": [
		"custom-mob-spawns.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.11.3",
		"minecraft": ">=1.19.3 <=1.20.5",
		"java": ">=17",
		"cloth-config2": ">=9.0.94"
	},
	"suggests": {
		"fabric-api": "*",
		"modmenu": ">=5.0.0"
	},
	"breaks": {
		"customspawns": "*"
	}
}